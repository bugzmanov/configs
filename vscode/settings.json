{
    "workbench.colorTheme": "Tomorrow Night Blue",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.minimap.enabled": false,
    "editor.lineNumbers": "relative",
    "editor.columnSelection": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.maxComputationTime": 0,
    "vim.easymotion": true,
    "vim.surround": true,
    "vim.replaceWithRegister": true,
    "vim.incsearch": true,
    "vim.ignorecase": true,
    // "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.sneak": true,
    "vim.highlightedyank.enable": true,
    "vim.visualstar":true,
    "vim.insertModeKeyBindings": [
      {
        "before": ["j", "j"],
        "after": ["<Esc>"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "P"],
        "after": ["O", "<Esc>", "p" ]
      },
      {
        "before": ["<leader>", "p"],
        "after": ["o", "<Esc>", "p" ]
      },
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<Esc>"],
        "commands": [":nohl"]
      },
      {
          "before": ["<C-l>"],
          "after": ["$"]
      },
      {
          "before": ["<C-h>"],
          "after": ["^"]
      },
      {
        "before":["<leader>","d"],
        "after": ["\"","_","d"]
      },
      {
        "before":["<leader>","y"],
        "after": ["\"","+","y"]
      },
      {
        "before":["<leader>","p"],
        "after": ["\"","+","p"]
      }

    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
      "<C-a>": false,
      "<C-f>": false
    },
    "diffEditor.renderSideBySide": true,
    "vim.easymotionMarkerBackgroundColor": "black",
    "[rust]": {
      "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "vim.otherModesKeyBindingsNonRecursive": [
      {
          "before": ["<C-w>", "+"],
          "after": [],
          "commands": [
              {"command": "workbench.action.increaseViewSize", "args": []}
          ]
      },
      {
          "before": ["<C-w>", "="],
          "after": [],
          "commands": [
              {"command": "workbench.action.increaseViewSize", "args": []}
          ]
      },
      {
          "before": ["<C-w>", "-"],
          "after": [],
          "commands": [
              {"command": "workbench.action.decreaseViewSize", "args": []}
          ]
      },
    ],
    "window.zoomLevel": 0,
    "omnisharp.useGlobalMono": "always",
    "files.exclude": {
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true
    },
    "files.watcherExclude": {
      "**/.bloop": true,
      "**/.metals": true,
      "**/.ammonite": true
    }
  }
